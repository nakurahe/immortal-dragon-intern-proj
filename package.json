{
    "name": "news-analyzer",
    "version": "1.0.0",
    "description": "AI-powered news analysis application",
    "scripts": {
        "setup": "bash setup.sh",
        "server": "cd server && npm run dev",
        "client": "cd client && npm start",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "install:all": "npm install && cd server && npm install && cd ../client && npm install"
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "concurrently": "^8.2.0"
    },
    "dependencies": {
        "newsapi": "^2.4.1"
    }
}
